{
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "destructuring": true,
      "classes": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "commonjs": true,
    "node": true
  },
  "globals": {
    "TEST_MODE": true
  },
  "rules": {
    "no-console": 0
  },
  "extends": "eslint:recommended"
}
